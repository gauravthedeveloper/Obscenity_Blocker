{
    "name": "Block Obscene Websites",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "http://*/*"
      
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
     
      "default_popup": "popup.html"
    }
  }
  