{
    "name": "Block Obscene Websites",
    "version": "1.0",
    "manifest_version": 2,
    "description": "This extension blocks Obscene Websites not suitable for children under 18",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "http://*/*",
      "https://*/*"
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
     
     
      "default_popup": "popup.html"
    }
  }
  